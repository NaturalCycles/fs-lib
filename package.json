{
  "name": "@naturalcycles/fs-lib",
  "version": "2.6.2",
  "description": "CLI and Node.js API to copy files with globs, promises, typescript and stuff.",
  "scripts": {
    "kpy": "tsn ./src/bin/kpy.ts",
    "kpy-debug": "tsn ./src/bin/kpy.ts src dist",
    "del": "tsn ./src/bin/del.ts",
    "del-debug": "tsn ./src/bin/del.ts dist --verbose --debug"
  },
  "dependencies": {
    "@naturalcycles/js-lib": "^10.0.0",
    "@naturalcycles/nodejs-lib": "^9.1.1",
    "@naturalcycles/time-lib": "^1.5.0",
    "cp-file": "^9.0.0",
    "fs-extra": "^9.0.0",
    "globby": "^11.0.0",
    "loud-rejection": "^2.1.0",
    "move-file": "^1.2.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@naturalcycles/dev-lib": "^10.3.0",
    "@types/node": "^13.1.6",
    "jest": "^25.1.0",
    "patch-package": "^6.2.1",
    "weak-napi": "^1.0.3"
  },
  "files": [
    "dist",
    "src",
    "!src/test",
    "!src/**/*.test.ts",
    "!src/**/__snapshots__",
    "!src/**/__exclude"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "del": "dist/bin/del.js",
    "kpy": "dist/bin/kpy.js"
  },
  "engines": {
    "node": ">=10.13"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NaturalCycles/fs-lib"
  },
  "author": "Kirill Groshkov",
  "license": "MIT"
}
