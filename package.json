{
  "name": "@naturalcycles/fs-lib",
  "version": "2.4.0",
  "description": "CLI and Node.js API to copy files with globs, promises, typescript and stuff.",
  "scripts": {
    "kpy": "tsn ./src/bin/kpy.ts",
    "kpy-debug": "tsn ./src/bin/kpy.ts src dist",
    "del": "tsn ./src/bin/del.ts",
    "del-debug": "tsn ./src/bin/del.ts dist --verbose --debug"
  },
  "dependencies": {
    "@naturalcycles/js-lib": "^8.4.0",
    "ansi-colors": "^4.1.1",
    "cp-file": "^7.0.0",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "loud-rejection": "^2.1.0",
    "move-file": "^1.2.0",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@naturalcycles/dev-lib": "^7.16.2",
    "@types/node": "^12.7.2",
    "jest": "^24.9.0"
  },
  "files": [
    "dist",
    "src",
    "!src/test",
    "!src/**/*.test.ts",
    "!src/**/__snapshots__",
    "!src/**/__exclude"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "del": "dist/bin/del.js",
    "kpy": "dist/bin/kpy.js"
  },
  "engines": {
    "node": ">=10.13"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NaturalCycles/kpy"
  },
  "author": "Kirill Groshkov",
  "license": "MIT"
}
